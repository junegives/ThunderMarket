<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.spring.market.mappers">

	<select id="product" resultType="com.spring.market.domain.productDTO">
		select
			bno, img, content, price, writer, pword, regDate, viewCnt, replyCnt
		from market
	</select>
	
</mapper>